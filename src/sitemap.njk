permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- for item in collections.sitemap %}
    <url>
        <loc>{{ item.urlAbsolute }}</loc>
        <lastmod>{{ item.dateModified | date }}</lastmod>
    </url>
    {%- endfor %}
</urlset>